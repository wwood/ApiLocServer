<h1>Listing expression_contexts</h1>

<table>
  <tr>
  </tr>

<% for expression_context in @expression_contexts %>
  <tr>
    <td><%= link_to 'Show', expression_context %></td>
    <td><%= link_to 'Edit', edit_expression_context_path(expression_context) %></td>
    <td><%= link_to 'Destroy', expression_context, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

